<div class="container" id="calender-container">
  <div class='well'>
    
    <h1>Lunch Menu</h1>
    
    <div id="menu-items">
      <h2 id="month">
        <%= link_to "<", date: @date.prev_month %>
        <%= @date.strftime("%B %Y") %>
        <%= link_to ">", date: @date.next_month %>
      </h2>
      
      <%= calendar do |date| %>
        <%= date.day %>
      <% end %>
    </div>
  </div>
</div>